        <location file="C:/_dev/OpenU/Audacity/audacity/lib-src/ffmpeg/libavutil/channel_layout.h" line="78" column="9" bodyfile="C:/_dev/OpenU/Audacity/audacity/lib-src/ffmpeg/libavutil/channel_layout.h" bodystart="78" bodyend="-1"/>
      </memberdef>
